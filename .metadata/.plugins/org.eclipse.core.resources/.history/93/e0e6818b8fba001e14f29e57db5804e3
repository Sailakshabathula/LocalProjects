package countMatchingChar;

public class DigitsCount {
	
	public static void main(String args[])
	{
		
	/*	123->
		
		123%10=3
		123/10=12-count=1
		
		n=n/10
		12%10=2
		12/10=1count=count+1=2
		
		n=n/10;
		1%10=1
	  1/10=0count=count+1=3*/
		
		int n=1235678;
		int count=0;
		while(n>0)
		{
			count=count+1;
			n=n/10;
		}
	  System.out.println(count);
	  
	  
	  
		
	}

}
